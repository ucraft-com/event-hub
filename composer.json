{
  "name": "ucraft-com/event-hub",
  "description": "EventHub - A simple Laravel package.",
  "type": "library",
  "readme": "README.md",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Ghukasyan",
      "email": "vladimir@ucraft.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ucraft-com/kafka-producer": "^2.0.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.5.10",
    "mockery/mockery": "^1.5",
    "ext-pcntl": "*",
    "ext-rdkafka": "*",
    "symfony/serializer": "7.0",
    "symfony/property-access": "7.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": false,
  "autoload": {
    "psr-4": {
      "Uc\\EventHub\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Uc\\EventHub\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Uc\\EventHub\\EventHubServiceProvider"
      ]
    }
  }
}
